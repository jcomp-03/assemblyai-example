// import module
const { assembly } = require("./axios");

function getTranscript(transcriptId, status) {
  // console.log("inside getTranscript function");
  assembly
    .get(`/transcript/${transcriptId}`)
    .then((res) => {
      status === "error"
        ? console.log('Transcription request failed:', res.data.error)
        : console.log(res.data);
    })
    .catch((err) => console.error("****Error getting transcript****", err));
}

module.exports = getTranscript;
